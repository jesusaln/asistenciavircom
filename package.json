{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "laravel": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "laravel:vite": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "start": "php artisan config:clear && php artisan optimize:clear && concurrently \"npm run laravel\" \"npm run dev\"",
    "serve": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "laravel-only": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "watch": "vite build --watch",
    "production": "npm run build && npm run laravel-only",
    "test": "php artisan test"
  },
  "devDependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "concurrently": "^8.2.2",
    "laravel-vite-plugin": "^1.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "vue": "^3.3.13"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
