{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "laravel": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "laravel:vite": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "start": "php artisan config:clear && php artisan optimize:clear && concurrently \"npm run laravel\" \"npm run dev\"",
    "serve": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "laravel-only": "php -d post_max_size=100M -d upload_max_filesize=100M artisan serve --host=0.0.0.0 --port=8001",
    "watch": "vite build --watch",
    "production": "npm run build && npm run laravel-only",
    "test": "php artisan test",
    "test:cliente": "php artisan test --filter=Cliente",
    "test:fresh": "php artisan migrate:fresh --seed",
    "test:cliente:fresh": "php artisan migrate:fresh --seed && php artisan test --filter=Cliente"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@inertiajs/vue3": "^2.3.6",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "axios": "^1.7.4",
    "concurrently": "^9.1.2",
    "laravel-vite-plugin": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0",
    "vue": "^3.5.26"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "^7.4.3",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
