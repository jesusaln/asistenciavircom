    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.4",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "notyf": "^3.10.0",
    "sweetalert2": "^11.26.17",
    "vue-chartjs": "^5.3.3",
